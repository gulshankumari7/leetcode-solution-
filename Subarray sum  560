class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int> prefixsum;
        prefixsum[0]=1;
        int sum=0,count=0;
        for(int num:nums){
            sum+=num;
            if(prefixsum.find(sum-k)!=prefixsum.end()){
                count += prefixsum[sum-k];
            }
            prefixsum[sum]++;
        }
        return count;
}
};
